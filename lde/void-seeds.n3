@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix scovo: <http://purl.org/NET/scovo#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix seed: <void-seeds.rdf#> .

## from http://richard.cyganiak.de/2007/10/lod/  AND http://esw.w3.org/topic/TaskForces/CommunityProjects/LinkingOpenData/DataSets

# DBpedia - the Wikipedia content as linked data
seed:DBpedia a void:Dataset ; 
			rdfs:label "DBpedia" ;
			foaf:homepage <http://dbpedia.org/> ;
        	dc:subject <http://dbpedia.org/resource/DBpedia> ;
			void:containsLinks seed:DBpedia2revyu ;
			void:containsLinks seed:DBpedia2uscensus ;		
			void:containsLinks seed:DBpedia2DBLP ;	
			void:statItem [
				rdf:value 109750000 ;
				scovo:dimension void:numOfTriples .
			] .

# outgoing DBpedia interlinking
seed:DBpedia2revyu a void:Linkset ;
			void:target seed:revyu .

seed:DBpedia2uscensus a void:Linkset ;
				void:target seed:uscensus .		
					
seed:DBpedia2DBLP a void:Linkset ;
			void:target seed:DBLP .			

# revyu.com
seed:revyu a void:Dataset ; 
			rdfs:label "Revyu.com" ;
			foaf:homepage <http://revyu.com/> ;
        	dc:subject <http://dbpedia.org/resource/Rating> ;
        	dc:subject <http://dbpedia.org/resource/Review> ;
			void:statItem [
				rdf:value 20000 ;
				scovo:dimension void:numOfTriples .
			].

# US census data
seed:uscensus a void:Dataset ; 
	    rdfs:label "2000 US Census" ;
      	foaf:homepage <http://www.rdfabout.com/demo/census/> ;
      	dc:subject <http://dbpedia.org/resource/Statistics> ;
      	dc:subject <http://dbpedia.org/resource/United_States> ;
		void:containsLinks seed:uscensus2Geonames ;
		void:statItem [
			rdf:value 2000000000 ;
			scovo:dimension void:numOfTriples .
		].

# outgoing US census interlinking
seed:uscensus2Geonames a void:Linkset ;
				void:target seed:Geonames .
				
# Eurostat data
seed:riese a void:Dataset ; 
			rdfs:label "riese" ;
      		foaf:homepage <http://riese.joanneum.at/> ;
      		dc:subject <http://dbpedia.org/resource/Statistics> ;
      		dc:subject <http://dbpedia.org/resource/Europe> ;
			void:statItem [
				rdf:value 3000000 ;
				scovo:dimension void:numOfTriples .
			] .

# DBLP - linked dataset of the DBLP Bibliography Database 
seed:DBLP a void:Dataset ; 
			rdfs:label "DBLP" ;
			foaf:homepage <http://www4.wiwiss.fu-berlin.de/dblp/all> ;
      		dc:subject <http://dbpedia.org/resource/Computer_science> ; 
      		dc:subject <http://dbpedia.org/resource/Journal> ;
      		dc:subject <http://dbpedia.org/resource/Proceedings> ;
			void:statItem [
				rdf:value 28000000 ;
				scovo:dimension void:numOfTriples .
			] .

# Geonames - linked dataset with geolocation data
seed:Geonames a void:Dataset ; 
			rdfs:label "GeoNames" ;
			foaf:homepage <http://sws.geonames.org/> ;
      		dc:subject <http://dbpedia.org/resource/Location> ;
			void:statItem [
				rdf:value 93000000 ;
				scovo:dimension void:numOfTriples .
			] .
      
# WordNet
seed:Wordnet a void:Dataset ; 
			rdfs:label "WordNet" ;
			foaf:homepage <http://www.w3.org/TR/wordnet-rdf/> ;
      		dc:subject <http://dbpedia.org/resource/WordNet> ;
			void:statItem [
				rdf:value 710000 ;
				scovo:dimension void:numOfTriples .
			] .
      
# Wikicompany
seed:Wikicompany a void:Dataset ; 
			rdfs:label "Wikicompany (en)" ;
			foaf:homepage <http://dbpedia.openlinksw.com/wikicompany/resource/Wikicompany> ;
      		dc:subject <http://dbpedia.org/resource/Business> .                      

# Jamendo
seed:Jamendo a void:Dataset ; 
			rdfs:label "Jamendo" ;
			foaf:homepage <http://dbtune.org/jamendo/> ;
			dc:subject <http://dbpedia.org/resource/Music> ;
			void:containsLinks seed:Jamendo2Geonames ;
			void:statItem [
				rdf:value 1100000 ;
				scovo:dimension void:numOfTriples .
			] .
			
# outgoing US census interlinking
seed:Jamendo2Geonames a void:Linkset ;
					void:target seed:Geonames .			